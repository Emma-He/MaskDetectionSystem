   "source": [
    "import tensorflow as tf\n",
    "\n",
    "# Convert the model\n",
    "path = 'model2'\n",
    "converter = tf.lite.TFLiteConverter.from_saved_model(path) # path to the SavedModel directory\n",
    "tflite_model = converter.convert()\n",
    "\n",
    "# Save the model.\n",
    "with open('model.tflite', 'wb') as f:\n",
    "  f.write(tflite_model)"
   ]
  }
